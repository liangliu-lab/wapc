{
    "unset_ssid":{
        "enable":true,
        "configure":true,
        "commands":[
            {
                "command":"no dot11 ssid ",
                "param":"",
                "type": "ssid"
            },
            {
                "configure" : "end",
                "command":"write mem"
            }
        ]
    },
    "set_channel":{
        "enable":true,
        "configure":true,
        "commands":[
            {
                "command":"interface Dot11Radio",
                "param":"",
                "type": "interface",
                "regex":"0|1"
            },
            {
                "command":"channel ",
                "param":"",
                "type": "channel"
            },
            {
                "configure" : "end",
                "command":"write mem"
            }
        ]
    },
    "set_ip" : {
        "enable" : true,
        "configure" : true,
        "commands" : [
            {
                "command" : "interface BVI1 "
            },
            {
                "command" : "ip address ",
                "param" : "",
                "type": "ip"
            },
            {
                "configure" : "end",
                "command":"write mem"
            }
        ]
    },
    "set_maxclients":{
        "enable":true,
        "configure":true,
        "commands":[
            {
                "command":"dot11 ssid ",
                "param":"",
                "type": "ssid"

            },
            {
                "command" : "max-associations ",
                "param" : "",
                "type": "maxclient"
            },
            {
                "configure" : "end",
                "command":"write mem"
            }
        ]
    },
    "set_ssid":{
        "enable":true,
        "configure":true,
        "pre":"unset",
        "commands":[
            {
                "command":"dot11 ssid ",
                "param":"",
                "type": "ssid"
            },
            {
                "command":"authentication open eap eap_methods"
            },
            {
                "command":"authentication network-eap eap_methods"
            },
            {
                "command":"authentication key-management wpa"
            },
            {
                "command":"information-element ssidl advertisement"
            },
            {
                "command":"exit"
            },
            {
                "command":"interface Dot11Radio",
                "param" :"",
                "type": "interface"
            },
            {
                "command":"ssid ",
                "param":"",
                "type": "ssid"
            },
	        {
                "configure" : "end",
                "command":"write mem"
            }
        ]
    },
    "set_permanent":{
        "enable":true,
        "configure":false,
        "commands":[
            {
                "command":"write mem"
            }
        ]
    },
    "show_ip" : {
        "enable" : true,
        "configure" : false,
        "commands" : [
            {
                "command" : "show interfaces BVI 1 | include Internet address"
            }
        ]
    },


    "show_cpu" : {
        "enable" : true,
        "configure" : false,
        "commands" : [
            {
                "command" : "show processes | include CPU utilization "
            }
        ]
    },

    "show_memory" : {
        "enable" : true,
        "configure" : false,
        "commands" : [
            {
                "command" : "show memory stat"
            }
        ]
    },
    "show_channel":{
